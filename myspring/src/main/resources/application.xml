<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="student" class="com.demo.bean.Student">
        <property name="name" value="laoye"/>
        <property name="age" value="1"/>
    </bean>
	<bean id="person" class="com.demo.Person" name="person2;person2">
		<constructor-arg name="name" value="jx"/>
	</bean>

	<alias name="person" alias="person3"/>
	<import resource="classpath:application1.xml"/>

</beans>